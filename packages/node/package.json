{
  "name": "@exceptionless/node",
  "version": "2.0.0-pre",
  "description": "JavaScript client for Exceptionless",
  "author": {
    "name": "exceptionless",
    "url": "https://exceptionless.com"
  },
  "keywords": [
    "exceptionless",
    "error",
    "feature",
    "logging",
    "tracking",
    "reporting",
    "node"
  ],
  "repository": {
    "url": "git://github.com/exceptionless/Exceptionless.JavaScript.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/exceptionless/Exceptionless.JavaScript/issues"
  },
  "private": true,
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "eslint --cache --ext .ts",
    "lint:fix": "eslint --fix",
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.esm.json -w --preserveWatchOutput",
    "test": "mocha --colors --require ts-node/register ./test --exit"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/mocha": "8.0.4",
    "@types/mock-fs": "4.13.0",
    "@types/sinon": "9.0.8",
    "@types/stack-trace": "0.0.29",
    "chai": "4.2.0",
    "mocha": "8.2.1",
    "mock-fs": "4.13.0",
    "sinon": "9.2.1"
  },
  "dependencies": {
    "@exceptionless/core": "2.0.0-pre",
    "stack-trace": "0.0.10"
  }
}
