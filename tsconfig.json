{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
          "@exceptionless/angularjs/*": ["packages/angularjs/src/*"],
          "@exceptionless/browser/*": ["packages/browser/src/*"],
          "@exceptionless/core/*": ["packages/core/src/*"],
          "@exceptionless/node/*": ["packages/node/src/*"],
          "@exceptionless/node-submit-sync/*": ["packages/node-submit-sync/src/*"],
          "@exceptionless/universal/*": ["packages/universal/src/*"]
        },
        "declaration": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "removeComments": true,
        "sourceMap": true,
        "target": "es5"
    }
}
